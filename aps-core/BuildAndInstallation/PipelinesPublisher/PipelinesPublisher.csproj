  <ItemGroup>
    <Compile Remove="Builders\Paths.cs" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\SolutionMiscFiles\AssemblyInfoShared.cs" Link="AssemblyInfoShared.cs" />
