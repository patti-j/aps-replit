  <ItemGroup>
    <None Include="BuildPaths.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="PT.AzureSignTool" Version="12.3.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\CommonProjects\Common\Common.csproj" />
  </ItemGroup>
</Project>
